{
  "author": "Group 09",
  "name": "FanCine",
  "version": "1.0.0",
  "description": "Movie ticking website",
  "target": "web",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "gulp serve",
    "start": "node --experimental-modules  --experimental-json-modules server.js",
    "lint:sass-fix": "sass-lint-auto-fix -c .sass-lint-auto-fix.yml",
    "lint:sass": "sass-lint -c .sass-lint.yml -v -q",
    "lint:js-fix": "eslint --fix",
    "lint:js": "eslint",
    "prettier": "prettier --write server public/njs"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:js-fix",
      "yarn lint:js",
      "yarn prettier"
    ],
    "*.scss": [
      "yarn lint:sass-fix",
      "yarn lint:sass"
    ],
    "*": [
      "git add ."
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.0.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "googleapis": "^66.0.0",
    "gulp-cli": "^2.3.0",
    "handlebars": "^4.7.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.14",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "rootpath": "^0.1.2",
    "swagger-ui-express": "^4.1.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.13",
    "cssnano": "^4.1.10",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-eslint-if-fixed": "^1.0.0",
    "gulp-file-cache": "^0.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-postcss": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-lint": "^1.4.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "postcss": "^8.1.10",
    "prettier": "^2.2.0",
    "sass-lint-auto-fix": "^0.21.2"
  },
  "engines": {
    "node": "^10 || ^12 || >=14"
  },
  "browserslist": [
    "last 2 versions",
    "> 5%"
  ],
  "license": "MIT",
  "private": true
}
