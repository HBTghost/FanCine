{
  "parser": "babel-eslint",
  "plugins": ["prettier", "import"],
  "extends": [
    "prettier",
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "quote-props": 0,
    "no-await-in-loop": 0,
    "no-unused-vars": 0,
    "max-len": 1,
    "no-throw-literal": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "consistent-return": 0,
    "prefer-const": 1,
    "no-undef": 1,
    "object-curly-newline": 0,
    "operator-linebreak": 0,
    "no-alert": 0,
    "no-restricted-globals": 1
  }
}